{
  "name": "@home/alerts",
  "version": "0.3.9",
  "private": true,
  "author": "Mariusz Kabala <mariusz@kabala.waw.pl>",
  "main": "./dist/index.js",
  "license": "ISC",
  "scripts": {
    "build": "rollup -c ../../rollup.config.js",
    "build-declarations": "tsc --declaration --emitDeclarationOnly --out dist/index.d.ts",
    "watch": "rollup -w -c ../../rollup.config.js",
    "dev": "concurrently \"yarn workspace @home/alerts watch\" \"nodemon dist/index.js\"",
    "start": "node dist/index.js",
    "prepare": "exit 0"
  },
  "dependencies": {
    "@home/commons": "0.7.0",
    "@home/logger": "0.1.3",
    "@home/mqtt": "0.8.1"
  }
}
