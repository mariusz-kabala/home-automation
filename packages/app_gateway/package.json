{
  "name": "@home/gateway",
  "version": "0.1.0",
  "author": "Mariusz Kabala <mariusz@kabala.waw.pl>",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "build-declarations": "tsc --declaration --emitDeclarationOnly --out dist/index.d.ts && node ../../scripts/fixDeclaration.js",
    "watch": "rollup -w -c ./rollup.config.js",
    "dev": "concurrently \"yarn workspace @home/gateway watch\" \"nodemon dist/index.js\"",
    "start": "node dist/index.js",
    "prepare": "exit 0",
    "create-migration": "migrate create",
    "migrate": "migrate"
  },
  "peerDependencies": {
    "graphql": "^15.5.0"
  },
  "dependencies": {
    "@home/commons": "0.6.0",
    "@home/mongoose-client": "0.2.0",
    "@typegoose/typegoose": "^9.10.1",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.9.0",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^15.5.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-graphql": "^1.1.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "^0.0.5"
  }
}
