{
    "name": "@home/ws2mqtt",
    "version": "0.1.2",
    "private": true,
    "author": "Mariusz Kabala <mariusz@kabala.waw.pl>",
    "main": "./dist/index.js",
    "scripts": {
        "build": "rollup -c ../../rollup.config.js",
        "build-declarations": "tsc --declaration --emitDeclarationOnly --out dist/index.d.ts",
