{
    "name": "@home/ws2mqtt",
    "version": "0.6.3",
    "private": true,
    "author": "Mariusz Kabala <mariusz@kabala.waw.pl>",
    "main": "./dist/index.js",
    "scripts": {
        "build": "rollup -c ../../rollup.config.js",
        "build-declarations": "tsc --declaration --emitDeclarationOnly --out dist/index.d.ts",
        "watch": "rollup -w -c ../../rollup.config.js",
        "dev": "concurrently \"yarn workspace @home/ws2mqtt watch\" \"nodemon dist/index.js\"",
        "start": "node dist/index.js",
        "prepare": "exit 0"
    },
    "devDependencies": {
        "@types/node-cron": "^2.0.2",
        "@types/ws": "7.2.1"
    },
    "dependencies": {
        "@home/deconz-api": "0.1.2",
        "@home/logger": "0.1.1",
        "@home/mqtt": "0.6.0",
        "node-cron": "^2.0.3",
        "ws": "^7.2.1"
    }
}
